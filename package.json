{
  "name": "scraper",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:service": "nest build $SERVICE && cp ./package.json ./dist/apps/$SERVICE/ && cp ./.env ./dist/apps/$SERVICE/",
    "build:users": "SERVICE=users yarn run build:service",
    "build:scraper": "SERVICE=scraper yarn run build:service",
    "build:all": "concurrently --max-processes 7 \"yarn run build:users\" \"yarn run build:scraper\"",
    "format": "prettier --write ./**/*.ts --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint ./**/*.ts --fix",
    "test": "jest --no-cache --config jest.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --config jest.json --coverage --no-cache --runInBand --logHeapUsage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "audit": "yarn run audit-ci --config ./audit-ci.json"
  },
  "dependencies": {
    "@fastify/static": "^6.8.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.3.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.3",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.3.3",
    "@nestjs/platform-fastify": "^9.3.3",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "audit-ci": "^6.6.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^7.6.0",
    "dayjs": "^1.11.7",
    "ejs": "^3.1.8",
    "mysql2": "^3.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "danger": "^11.2.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "rimraf": "^4.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.9.5"
  }
}
